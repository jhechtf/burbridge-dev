{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "plugin:vue/essential",
    "@vue/eslint-config-typescript/recommended",
    "eslint:recommended"
  ],
  "parser": "vue-eslint-parser",
  "rules": {
    "vue/max-attributes-per-line": [
      "warn",
      {
        "singleline": 9
      }
    ],
    "vue/multi-word-component-names": ["off"],
    "vue/no-reserved-component-names": ["off"]
  },
  "root": true
}